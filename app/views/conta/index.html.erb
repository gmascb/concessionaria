<p id="notice"><%= notice %></p>

<h1>Listing Conta</h1>

<table>
  <thead>
    <tr>
      <th>Vencimento</th>
      <th>Emissao</th>
      <th>Ficha</th>
      <th>Customer</th>
      <th>Historico</th>
      <th>Datapagamento</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conta.each do |contum| %>
      <tr>
        <td><%= contum.vencimento %></td>
        <td><%= contum.emissao %></td>
        <td><%= contum.Ficha %></td>
        <td><%= contum.Customer %></td>
        <td><%= contum.historico %></td>
        <td><%= contum.datapagamento %></td>
        <td><%= contum.status %></td>
        <td><%= link_to 'Show', contum %></td>
        <td><%= link_to 'Edit', edit_contum_path(contum) %></td>
        <td><%= link_to 'Destroy', contum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contum', new_contum_path %>
