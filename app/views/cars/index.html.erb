<p id="notice"><%= notice %></p>

<h1>Carros</h1>
<table>
  <thead>
    <%= stylesheet_link_tag  'scaffolds', media: 'all', 'data-turbolinks-track' => true %>
    <tr>
      <th>Status</th>
      <th>Local</th>
      <th>Placa</th>
      <th>Modelo</th>
      <th>Combustivel</th>
      <th>Anofabricacao</th>
      <th>Andomodelo</th>
      <th>Km</th>
      <th>Valorvenda</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.status %></td>
        <td><%= car.local.descricao %></td>
        <td><%= car.placa %></td>
        <td><%= car.modelo %></td>
        <td><%= car.combustivel %></td>
        <td><%= car.anofabricacao %></td>
        <td><%= car.anomodelo %></td>
        <td><%= car.km %></td>
        <td><%= car.valorvenda %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
