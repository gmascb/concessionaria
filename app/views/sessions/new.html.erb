<head>
    <IMG class="displayed" src="https://www.benaiamotors.com.br/sites/default/files/logo.png" alt="logo"><br />
    <%= stylesheet_link_tag    'sessions', media: 'all', 'data-turbolinks-track' => true %>
</head>
<body>
  <div class="form">
    <h1>Log in</h1>
      <%= form_tag sessions_path do%>
        <div class = "field">
          <%= text_field_tag :email, nil, placeholder: 'E-mail' %>
        </div>
        <div class = "field">
          <%= password_field_tag :password, nil, placeholder: 'Senha' %>
        </div>
        <div class="actions">
            <%= submit_tag "Log In", :id => 'loginbtn' %>
        </div>
        <% end %>
    </div>
</body>
    

